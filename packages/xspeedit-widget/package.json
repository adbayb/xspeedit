{
  "name": "xspeedit-widget",
  "private": true,
  "version": "0.1.0",
  "description": "Application web consommant la librarie xspeedit",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf ./dist ./out",
    "dev": "next",
    "build": "next build",
    "start": "npm run build && next start",
    "export": "npm run build && next export",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
	"deploy:styleguide": "npm run styleguide:build && cp -R ./static ./styleguide && surge --project ./styleguide --domain xspeedit-styleguide.surge.sh",
	"deploy:app": "npm run export && surge --project ./out --domain xspeedit.surge.sh",
	"deploy": "npm run deploy:app && npm run deploy:styleguide"
  },
  "author": "Ayoub Adib <adbayb@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "next": "4.1.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-styleguidist": "6.0.33",
    "xspeedit-core": "../xspeedit-core"
  },
  "devDependencies": {
    "babel-preset-react": "6.24.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-to-json": "3.2.2",
    "prop-types": "15.6.0",
    "rimraf": "2.6.2",
    "surge": "0.19.0"
  }
}
