{
  "name": "xspeedit-core",
  "private": true,
  "version": "0.1.0",
  "description": "Librairie core xspeedit permettant de calculer et maximiser le nombre d'article par carton",
  "main": "dist/xspeedit-core.node.js",
  "browser": "dist/xspeedit-core.browser.js",
  "module": "src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "npm run clean:build",
    "predoc": "npm run clean:doc",
    "clean:build": "rimraf ./dist",
    "clean:doc": "rimraf ./doc",
    "doc": "jsdoc ./src --destination ./doc --recurse",
    "build:node": "cross-env BABEL_ENV=node rollup --config",
    "build:browser": "cross-env BABEL_ENV=browser rollup --config",
    "build": "npm run build:node && npm run build:browser",
    "deploy": "npm run doc && surge --project ./doc --domain xspeedit-core.surge.sh",
    "watch": "cross-env BABEL_ENV=node rollup --config --watch",
    "dev": "npm run build:node && concurrently \"npm run watch\" \"nodemon --watch dist --exec npm start\"",
    "start": "node dist/xspeedit-core.node.js"
  },
  "author": "Ayoub Adib <adbayb@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.1",
    "jsdoc": "3.5.5",
    "nodemon": "1.12.1",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "2.6.2",
    "rollup": "0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-babel-minify": "3.1.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "surge": "0.19.0"
  },
  "engines": {
    "node": ">= 6.12.0"
  }
}
